.component_radio {
  height: 100rpx;
  border-bottom: 2rpx solid #f1f1f1;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  padding: 0 20rpx;
}

.component_radio_text {
  flex: 1;
}

.component_radio_image {
  width: 50rpx;
  height: 50rpx;
  margin-right: 20rpx;
}

.component_radio_icon {
  flex: 1;
  text-align: right;
}
